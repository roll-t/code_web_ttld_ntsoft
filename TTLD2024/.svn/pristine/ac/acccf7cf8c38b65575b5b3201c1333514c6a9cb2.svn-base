    function SuaDuLieu(ID) {
        if (!QuyenSua()) {
            return false;
        }
        $('#lblTieuDeThemMoi').text('Cập nhật hộ gia đình');
        $('#mdThemMoiTiepTuc').modal('hide');
        $('#mdThemMoiHGD').modal('show');
        PhanQuyenComBoDiaBan('TinhID_TT', 'HuyenID_TT', 'XaID_TT', 'ThonID_TT');
        const result = NTS.getAjax('/DanhMuc/HoGiaDinh/LoadDuLieuSua', { id: ID });
        if (!result.Err && result.Result != null) {
            let data = result.Result[0];
            $('#HoGiaDinhID').value(data.HoGiaDinhID);
            $('#MaHo').value(data.MaHoGiaDinh);
            $('#HoVaTen').value(data.HoVaTenChuHo);
            $('#SoDinhDanh').value(data.SoCCCD);
            soCCCD = data.SoCCCD;
            $('#NgayCapSoDinhDanh').value(data.NgayCap);
            $('#NoiCap_TinhID').value(data.NoiCapID);
            $('#GioiTinhID').value(data.GioiTinhID);
            $('#NgaySinh').value(data.NgayThangNamSinh);
            $('#QuocTichID').value(data.QuocTichID);
            $('#DanTocID').value(data.DanTocID);
            $('#TonGiaoID').value(data.TonGiaoID);
            $('#SoDienThoai').value(data.SoDienThoai);
            $('#PhanLoaiHoID').value(data.PhanLoaiHoGDID);
            $('#Email').value(data.Email);
            $('#drop_GiayTo').attr('data-value', data.LoaiHoGD); //Loai ho gia dinh gia tri la "1": so ho khau, gia tri la "2": so tam tru
            if (data.LoaiHoGD == 1) {
                $('#drop_GiayTo').html(' Sổ hộ khẩu &nbsp; <i class="fa-solid fa-caret-down"></i>');
            } else {
                $('#drop_GiayTo').html(' Sổ tạm trú &nbsp; <i class="fa-solid fa-caret-down"></i>');
            }
            $('#drop_GiayTo').attr('data-value', data.LoaiHoGD);
            $('#SoGiayTo').value(data.SoGiayTo);
            $('#SoNha_TT').value(data.SoNhaTT);
            setTimeout(() => {
                $('#TinhID_TT').value(data.DiaBanHCID_TinhTT);
                $('#HuyenID_TT').value(data.DiaBanHCID_HuyenTT);
                $('#XaID_TT').value(data.DiaBanHCID_XaTT);
                $('#ThonID_TT').value(data.DiaBanHCID_ThonTT);
            }, 50);
            $('#DiaChiThuongTru').value(data.DiaChiCuTheTT);
            $('#TrangThai').value(data.TrangThai);
            $('#GhiChu').value(data.GhiChu);
            UpdateLabelDangSD('#TrangThai');
        }
        else result.CanhBao ? NTS.canhbao(result.Msg) : NTS.loi(result.Msg);
        tempthem = 'sua';
    }

    ///////// PHÍM TẮT /////////
    var hotKey = 0; // 1 thêm
    $(document).on('keydown', function (e) {
        console.log(e.keyCode);
        switch (e.keyCode) {
            case 113:
                if (hotKey == 0)
                    $('#btnThemMoi').trigger('click');
                e.preventDefault();
                break;
            case 114:
                if (hotKey == 0)
                    $('.nav-search-input').focus();
                e.preventDefault();
                break;
            case 115:
                if (hotKey == 1)
                    if ($('#mdThemMoiHGD').hasClass('show')) {
                        $('#mdThemMoiHGD').modal('hide');
                        e.preventDefault();
                        break;
                    } else if ($('#mdThemMoiThanhVien_us').hasClass('show')) {
                        $('#mdThemMoiThanhVien_us').modal('hide');
                        e.preventDefault();
                        break;
                    } else if ($('#mdThemMoi_TL').hasClass('show')) {
                        $('#mdThemMoi_TL').modal('hide');
                        e.preventDefault();
                        break;
                    }
            case 120:
                if (hotKey == 1) {
                    if ($('#mdThemMoiHGD').hasClass('show')) {
