                    }
                    else if ($('#mdThemMoi_TL').hasClass('show')) {
                        $('#btnLuuVaDong_TL').trigger('click');
                        e.preventDefault();
                        break;
                    }
                }
            case 121:
                if (hotKey == 1) {
                    if ($('#mdThemMoiHGD').hasClass('show')) {
                        $('#btnTiepTuc').trigger('click');
                        e.preventDefault();
                        break;
                    }
                }
        }
    });
    $(document).on('shown.bs.modal', '#mdThemMoiHGD', function () {
