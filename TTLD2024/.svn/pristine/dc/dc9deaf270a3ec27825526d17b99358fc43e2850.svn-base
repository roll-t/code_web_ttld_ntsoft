        display: flex;
        flex-wrap: wrap;
    }
    @@media (max-width: 1024px) {       
        .item_camnang-img {
            border: unset;
            box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.1);
        }
    }
    @@media (max-width: 992px) {
        .col-md-3 {
            width: 25%;
        }
