        color: white;
        font-weight: bold;
        font-size: 25px !important;
        min-width: 50px;
    }

    .opInfo {
        padding-left: 10px;
    }

    .listOption {
        background-color: white;
        height: 300px;
        overflow-y: scroll;
    }

    .text-diachi {
