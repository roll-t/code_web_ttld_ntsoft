<link href="~/css/Style_CTTViecLam.css?v=@DateTime.Now.Ticks.ToString()" rel="stylesheet" />
<style>
    .bg-ln-header {
        background: linear-gradient(135deg, #ff0000 50%, #f2ff0f 50%);
    }

    .custom-btn-login {
        margin-top: 5px;
        margin-bottom: 3px !important;
        border-radius: 5px !important;
    }

    img.custom-img-sld {
        max-width: 100px;
    }

    .is-3 {
        gap: 0.75rem;
    }

    .navbar-item {
        color: #fff;
        font-size: 14px;
        font-family: "Roboto", sans-serif;
        font-weight: 700;
        text-transform: uppercase;
    }

    .nav-link .minu {
        color: white !important;
    }

    .navbar-dropdown .navbar-item {
        color: black;
        font-weight: normal;
        text-transform: uppercase !important;
    }

    a:hover {
        text-decoration: none;
    }

    @@media (max-width: 900px) {
        .footer-1 {
            padding: 10px 20px;
        }

    }

    @@media (max-width: 1024px) {
        .tt-project, .btn-respone {
            display: none;
        }

        .box-nav {
            display: none;
        }

        .input-find {
            width: 100% !important;
        }

        .field {
            padding: 10px;
        }

        .tt-project {
            display: none
        }
    }


    @@media (min-width: 1024px) {
        .menu-mobile {
            display: none;
        }
    }

    .menu-mobile {
        margin-top: 8px;
        margin-bottom: -5px;
        z-index: 10;
    }

    #menuMobile {
        display: none;
        position: fixed;
        background: white;
        margin-left: 10px;
        padding: 10px;
        border-radius: 0 10px 10px 10px;
        z-index: 10;
    }

    ul.menu-list.menu-list-data {
        margin-left: 10px;
        border-left: 3px solid #d2d2d2;
    }
    .container, .container-fluid{
        padding: 0 !important;
    }
    .header{
        margin: 0 !important;
        padding-bottom:0;
    }
    .navbar .navbar-nav>li.open>a, .navbar .navbar-nav>li>a:focus, .navbar .navbar-nav>li>a:hover{
        background-color: unset !important;
        color: unset !important;
    }
    .breadcrumb a{
        display: inline;
    }
    /*Notifi*/
    .icon_notifi{
        margin-right: 15px;
        font-size: 20px;
        padding-top: 8px;
        color:var(--warning-color);
    }
    .number_notifi{
        position: absolute;
        top: -4px;
        left: -9px;
        width: 19px;
        height: 19px;
        background: #e21111;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 11px;
        color: #fff;
    }
    .notifi{
        text-align: center;
        color: #999;
        font-size: 14px;
    }
    .modal-backdrop{
        z-index:100;
    }
    .modal-dialog .table-header{
        border-radius:0;
    }

    .dropdown_notifi{
        position:relative;
    }

    .dropdown_notifi-list{
        position:absolute;
        top:45px;
        right:-20px;
        background:#fff;
        box-shadow:1px 2px 4px rgba(0,0,0,0.3);
        display:none;
    }
    .dropdown_notifi-list-item{
        min-width:300px;
        max-height:320px;
        border-bottom:1px solid #D9D9D9;
        overflow:auto;
    }
    .dropdown_notifi-list::before{
        content: "";
        position: absolute;
        top: -6px;
        right: 39px;
        margin-left: -5px;
        border-width: 0 5px 5px 5px;
        border-style: solid;
        border-color: transparent transparent #bdbdbd  transparent;
        z-index: 10;

    }

     .dropdown_notifi-list::after{
        content: "";
        position: absolute;
        top: -17px;
        right: 17px;
        margin-left: -5px;
        width: 53px;
        height: 20px;
        border-style: solid;
        border-color: transparent transparent transparent transparent;
        z-index: 10;
    }

    .icon-fa-circle{
        text-align:center;
    }

    .icon-fa-circle>i{
        text-align:center;
        color:var(--success-color);
        font-size:9px;
    }
    .notifi-title p{
        font-size: 13px;
        color: #000;
        line-height:17px;
        margin-top:2px;
        text-overflow: ellipsis;
        white-space: normal;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 3;
        overflow: hidden;
    }
     .notifi-title span{
         color:#999;
         font-size:12px;
     }

     .notifi_item{
         height:88px;
         padding:2px 2px;
         border-bottom:1px solid #D9D9D9;
     }
        .notifi_item:hover {
            background: #d3f6f0;
            cursor: pointer;
        }
     .icon-xmark{
         font-size:13px;
         color:#999;
     }
     .icon-xmark:hover{
         color:#868686;
         cursor:pointer;
     }

    .dropdown_notifi-list-item::-webkit-scrollbar {
        width: 8px; /* Độ rộng của thanh cuộn */
    }

    .dropdown_notifi-list-item::-webkit-scrollbar-track {
        background: #f1f1f1; /* Màu nền của vùng chứa thanh cuộn */
        border-radius:8px;

    }

    .dropdown_notifi-list-item::-webkit-scrollbar-thumb {
        background: #D9D9D9; /* Màu nền của thanh cuộn */
        border-radius:8px;
    }

    .dropdown_notifi-list-item::-webkit-scrollbar-thumb:hover {
        background: rgba(0,0,0,0.3); /* Màu nền của thanh cuộn khi hover */
        cursor:pointer;
    }
    .dropdown_notifi-btnXoa{
        text-align: center;
        padding: 6px 0;
    }
    .dropdown_notifi-btnXoa>span{
        font-size: 15px;
        color: #999;
    }
    .btnXoaThongBao:hover{
        background:#f9f5f5;
        cursor:pointer;
    }

    .dropdown_notifi:hover .dropdown_notifi-list{
        display:block;
        cursor:pointer;
    }
     .dropdown_notifi:hover .dropdown_notifi-list::before{
         display:bolock;
     }
    #modal_xemThongBao {
        z-index: 9999;
    }

     .btn-dadangnhap{
         border: 1px solid var(--primary-color) !important;
     }
     .btn-dadangnhap .icon-user{
         color:var(--primary-color) !important;
     }

    .btn-dadangnhap .dropdown-content{
        top: 25px;
        left: -100px;
    }
    .btn-dadangnhap .dropdown-content::before{
        left:168px;
    }

    .btn-dadangnhap:hover {
        background-color: var(--primary-color) !important;
    }

    .btn-dadangnhap:hover .icon-user{
        color:#FFF !important;
    }

     .btn-dadangnhap:hover span{
         color:#FFF !important;
     }
     .btn-groupzz.btn-dadangnhap{
         padding: 0 20px;
     }

    #menu-header {
        background-color: var(--primary-color) !important;
    }
    .badge_mucluong {
        width: 100px;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 1;
        overflow: hidden;
        background: #FFF;
    }
    .navbar-fixed-top{
        border-width:0 0 0;
    }
    #DiaDiem_usBanner + .select2-container--default .select2-selection--single {
        height: 40px !important;
        line-height: 40px !important;
    }

    #DiaDiem_usBanner + .select2-container--default .select2-selection--multiple {
        min-height: 40px !important;
    }

    #DiaDiem_usBanner + .select2-container--default .select2-selection__rendered {
        line-height: 40px !important;
    }

    #DiaDiem_usBanner + .select2-container--default .select2-dropdown {
        max-height: 200px !important;
        overflow-y: auto !important;
    }
    .header-right--login > div + div {
        margin-left: 0px;
    }
    .btn-dnhap {
        width: 100%;
        height: 100%;
        border-radius: 0px;
        background: var(--primary-color) !important;
        border: none;
        border-right: 1px solid #dbdbdb;
    }
    .btn-dky {
        width: 100%;
        height: 100%;
        border-radius: 0px;
        border: none;
    }
    .btn-dnhap:hover {
        background: #005094 !important;
    }
    .btn-dky:hover {
        background: #005094 !important;
    }
    .btn-dnhap .dropdown-content {
        top: 34px !important;
    }
    .btn-dky .dropdown-content {
        top: 34px !important;
    }
    .minu {
        color: white !important;
    }
    /* >= Tablet */
    @*@@media (min-width: 740px) {

    }
    @/* PC medium resolution > */
    @@media (min-width: 1113px) {

    }*@
    /* Tablet - PC low resolution */
