                }

        .search-box input[type=text] + span {
            z-index: -1;
            position: absolute;
            border: 2px solid #000;
            top: 0;
            width: 21px;
            height: 21px;
            transition: all .25s ease-in .25s;
            border-radius: 25px;
            left: 0
        }

            .search-box input[type=text] + span::before {
                transition: all .25s ease-in .5s;
                transform-origin: left top;
                content: '';
                position: absolute;
                width: 10px;
                height: 5px;
                border-radius: 5px;
                background: #000;
