    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@10.0.2/swiper-bundle.min.css">
    <script src="https://cdn.jsdelivr.net/npm/swiper@10.0.2/swiper-bundle.min.js"></script>
    <asp:ContentPlaceHolder ID="head" runat="server">
        <link href="../../Content/themes/Theme_1/NTS/css/Footer.css" rel="stylesheet" />
        <link href="~/Content/CongThongTin/lib/flickity.css" rel="stylesheet" />
    </asp:ContentPlaceHolder>
    <style>
        body {
            font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
            font-size: 12px;
            font-size-adjust: inherit;
            font-stretch: normal;
            font-variant: normal;
            font-style: normal;
        }

        .ntsloadding {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 9999;
            opacity: 0.5;
            text-align: center;
            background: #CCC;
            filter: alpha(opacity=80);
        }

        .message-loading-overlay > .ace-icon {
            position: absolute;
            top: 40%;
            left: 49%;
            right: 0;
            text-align: center;
        }

        .footer {
            height: 100% !important;
            width: 100% !important;
        }

        .main-content {
            font-size: 14px;
            background: white;
            margin-top: 2.7%;
        }

        .breadcrumb-container {
            background: #f5f5f5;
        }

        .breadcrumb {
            margin: 10px 0;
            font-size: 13px !important;
        }

        .GridData .tabulator-header .tabulator-col, .GridData .tabulator-header .tabulator-col-row-handle {
            background-color: #0d7dcd !important;
            color: #fff !important;
        }

        .form-label {
            font-weight: bold;
            font-size: 14px;
        }

        .khungvien legend {
            background: #0d7dcd;
            border-radius: 20px;
            color: #fff;
            padding: 0px 10px;
            font-size: 12px;
        }

        .modal-content-2 {
            border-radius: 0;
            -webkit-box-shadow: none;
            box-shadow: none;
            position: relative;
            background-color: #fff;
            border: 1px solid #999;
            border: 1px solid rgba(0,0,0,.2);
            border-radius: 6px;
            -webkit-box-shadow: 0 3px 9px rgba(0,0,0,.5);
            box-shadow: 0 3px 9px rgba(0,0,0,.5);
            outline: 0;
            background-clip: padding-box;
            font-size: 14px;
            border-radius: 8px 8px 0 0;
        }

        @@media (min-width: 768px) {
            .modal-content-2 {
                -webkit-box-shadow: 0 5px 15px rgba(0,0,0,.5);
                box-shadow: 0 5px 15px rgba(0,0,0,.5);
            }
        }

        .modal-dialog .table-header {
            border-radius: 8px 8px 0 0;
            background: #40a5ff !important;
            text-transform: uppercase !important;
        }

        .primary-color {
            background-color: #0d7dcd !important;
        }

        .GridData {
            font-size: 13px !important;
        }


        .btn-primary {
            padding: 7px 13px !important;
            background-color: var(--primary-color) !important;
            color: #fff !important;
            transition: 0.2s linear;
            cursor: pointer;
            border-radius: 3px;
            display: inline-block;
            min-width: 95px;
            text-align: center;
            border: 1px solid inherit;
        }

            .btn-primary:hover {
                background-color: #2a85d9 !important;
            }

        .btn-success {
            padding: 7px 13px !important;
            background-color: var(--success-color) !important;
            color: #fff !important;
            transition: 0.2s linear;
            cursor: pointer;
            border-radius: 3px;
            text-align: center;
            border: 1px solid inherit;
        }

            .btn-success:hover {
                background-color: #17a94f !important;
            }

        .btn-daungtuyen {
            background: #f3f3f3;
            color: var(--primary-color);
        }

        .btn-DaUngTuyen {
            background-color: #cfebff80 !important;
            color: var(--primary-color) !important;
            border-color: var(--primary-color) !important;
        }

            .btn-DaUngTuyen:hover {
                background-color: #cfebff80 !important;
            }

        .btn-DaLuuTin {
            color: #FFD43B !important;
        }

        .btn-luutru {
            cursor: pointer;
            font-size: 20px;
        }
    </style>
</head>
<body class="no-skin">
    <div id="ntsSecurity" runat="server"></div>
    <input type="hidden" value="0" runat="server" id="hdfWidthNTS" />
    <div class="ntsloadding" style="display: none;">
        <div style="margin-top: 20%; z-index: 9999">
            <div style="font-size: 38px; color: yellow;"><i class="fa fa-spinner fa-spin"></i></div>
            <div><span class="process" style="color: black;">Đang xử lý...</span></div>
        </div>
    </div>
    <div id="Loadding">
        <div class="message-loading-overlay"><img class="ace-icon" src="../../Images/loading.gif" width="48" height="48" alt="Vui lòng chờ..." /></div>
    </div>
    <input id="gritter-light" checked="" type="checkbox" class="ace ace-switch ace-switch-5" />
    @Html.Partial("HeaderCTTViecLam")
    <div class="main-container ace-save-state" id="main-container">
        <div class="main-content">
            <div class="main-content-inner" id="divNoiDung">
                @RenderBody()
            </div>
        </div>
        @Html.Partial("FooterCTTViecLam")
        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse" title="Lên đầu trang">
            <i class="fa fa-arrow-up" aria-hidden="true"></i>
        </a>
    </div>
    <script src="~/Content/TablerTheme/libs/js/flickity.pkgd.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/ace-extra.min.js"></script>
    <link href="~/Content/TablerTheme/libs/toastr/toastr.min.css" rel="stylesheet" />
    <script src="~/Content/TablerTheme/libs/toastr/toastr.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/bootstrap.min.js"></script>
    <script src="~/Content/TablerTheme/libs/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/TablerTheme/libs/js/jquery.dataTables.bootstrap.min.js"></script>
    <script src="~/Content/TablerTheme/material-preloader/js/materialPreloader.js"></script>
    <script src="~/Content/TablerTheme/js/Login/jquery.gritter.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/bootbox.js"></script>
    <script src="~/Content/TablerTheme/js/Login/select2.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/ace-elements.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/ace.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/dataTables.select.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/jquery-ui.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/jquery.mask.min.js" defer="defer"></script>
    <script src="~/Content/TablerTheme/js/Login/bootstrap-datepicker.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/bootstrap-timepicker.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/moment.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/bootstrap-datetimepicker.min.js"></script>
    <script src="~/Content/TablerTheme/js/Login/bootstrap-tag.min.js" defer="defer"></script>
    <script src="~/Content/TablerTheme/OwlCarousel/js/owl.carousel.min.js"></script>
    <script src="~/Scripts/DungChung/NTSLibrary.js?v=@DateTime.Now.Ticks.ToString()"></script>
    <script src="~/Scripts/DungChung/permission.js?v=@DateTime.Now.Ticks.ToString()"></script>
    <script type="text/javascript">
        setTimeout(function () {
            NTS.unloadding();
        }, 50)
    </script>
</body>
</html>
