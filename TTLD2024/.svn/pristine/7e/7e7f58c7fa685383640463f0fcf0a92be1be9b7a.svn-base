.frm-notisalagovapp{position:fixed;top:0;left:0;bottom:0;right:0;background:#80808069;z-index:99999;display:none}.frm-notisalagovapp .frm-notify{height:574px;width:464px;background:#fff;position:fixed;left:calc(50% - 232px);top:calc(50% - 287px);border-radius:10px;padding:40px;box-shadow:1px 1px 20px 2px #808080de}.frm-notisalagovapp .frm-notify .frm-content .noti-content{padding-bottom:calc(50% - 119px);font-size:14px;line-height:22px}.frm-notisalagovapp .frm-notify .frm-content .noti-content .app-name{font-size:14px}.frm-notisalagovapp .frm-notify .frm-content .frm-header{font-size:20px;text-align:center;font-weight:700;color:#35a5ff}.frm-notisalagovapp .frm-notify .frm-content .btn-continue{height:32px;width:140px;background-image:linear-gradient(0deg,#2f80ff 0%,#3ccbff 100%);border-radius:3px;text-align:center;box-shadow:0 2px 5px 0 rgba(0,0,0,.26);padding:8px;font-weight:700;color:#fff;cursor:pointer;margin-left:calc(50% - 70px);top:0}.frm-notisalagovapp .frm-notify .frm-content .btn-continue:hover{background:#3276e3!important}