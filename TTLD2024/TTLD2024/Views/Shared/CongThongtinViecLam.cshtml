    <link href="~/Content/TablerTheme/OwlCarousel/css/owl.theme.default.css" rel="stylesheet" />
    <script src="~/ckeditor/adapters/jquery.js"></script>
    <script src="~/Content/TablerTheme/libs/jquery/jquery.js"></script>
    <script src="~/Content/TablerTheme/libs/js/jquery.js"></script>
    <link href="~/Content/TablerTheme/libs/css/style_CTT.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/TablerTheme/Login/css/chosen.min.css" />
    <link href="~/Content/CongThongTin/lib/bulma/css/bulma.min.css" rel="stylesheet" />
    <script src="~/Content/TablerTheme/js/Login/jquery-2.1.4.min.js"></script>
    <script src="~/Content/TablerTheme/libs/js/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/customs.js?v=@DateTime.Now.Ticks.ToString()"></script>
    <link href="~/Content/TablerTheme/css/style-custom.css" rel="stylesheet" />
    <script src="~/Scripts/DungChung/NTSPlugin.js?v=@DateTime.Now.Ticks.ToString()"></script>
    <link href="~/Content/TablerTheme/libs/css/tabulator.min.css" rel="stylesheet" />
    <script src="~/Content/TablerTheme/libs/js/tabulator.min.js"></script>
    <script src="~/Content/TablerTheme/libs/tabulator/dist/js/tabulator.min.js"></script>
