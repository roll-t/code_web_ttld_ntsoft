<div id="modal_xemThongBao" class="modal" data-backdrop="static">
    <div class="modal-dialog modal-full">
        <div class="modal-content">
            <div class="modal-header" style="background: #16a085; color: #fff; display: flex; align-items: center; justify-content: space-between;">
