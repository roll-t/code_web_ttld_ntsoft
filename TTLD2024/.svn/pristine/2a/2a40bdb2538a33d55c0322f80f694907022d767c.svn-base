        }
    }
    #header-ctt_vieclam {
        position: fixed;
        left: 0;
        right: 0;
        top: 0;
        z-index: 100000
    }
    #main-menu .nav-item, #main-menu .nav-item > a, .header-js .group-btn-header, .flag--pc {
        height: 54px;
        line-height: 54px;
    }
    .header-js .header-inner {
        height: 54px;
    }
    .header-inner .logo.logo-nlg img.logo-vn {
        width: 48px;
        height: 48px;
    }
    .head-right, .header-right {
        height: 54px;
    }
    .navbar-collapse.collapse{
        margin-top:10px;
    }
    .header-right--login {
        margin-top: 10px;
    }
    span.minu {
        max-width: 190px;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 1;
        overflow: hidden;
    }
    #modal_trigger_mobile,
    #modal_trigger_mobile1 {
        display: flex;
        align-items: center;
    }
    #modal_trigger_mobile.icon-user{
        margin-top:4px;
    }
    .item_camnang-tieude-h3 {
        width: 100%;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 3;
        overflow: hidden;
    }
    .loaicamnang-trangchu {
        width: 100%;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 1;
        overflow: hidden;
    }
    #logo-ctt-header {
        margin-top: 0px !important;
        border-radius: 5px;
    }
</style>

