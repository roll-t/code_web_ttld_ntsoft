        top: 10px;
        left: -15px !important;
    }

    .box-item-news-work {
        margin-bottom: 1.5rem;
    }

    .panel.is-primary .panel-heading {
        background-color: #00d1b2;
        color: #fff;
        text-transform: uppercase;
    }

    .panel-block.is-active {
        border-left-color: #485fc7;
        color: #363636;
        text-transform: capitalize;
        font-size: 16px;
    }

    .panel-block {
